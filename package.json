{
  "name": "example-testcafe",
  "version": "1.0.0",
  "description": "Example testcafe project",
  "repository": "https://github.com/leecheneler/example-testcafe",
  "author": "leecheneler",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test:all": "testcafe chrome:headless,firefox,ie,safari src/fixtures/*.ts",
    "test:chrome": "testcafe chrome src/fixtures/*.ts",
    "test:chrome:headless": "testcafe chrome:headless src/fixtures/*.ts",
    "test:firefox": "testcafe firefox src/fixtures/*.ts",
    "test:ie": "testcafe ie src/fixtures/*.ts",
    "test:safari": "testcafe safari src/fixtures/*.ts",
    "test:chrome:emulation": "testcafe chrome:emulation:device=pixel 2 src/fixtures/*.ts"
  },
  "dependencies": {
    "@testing-library/testcafe": "^3.3.0",
    "testcafe": "^1.5.0"
  }
}
